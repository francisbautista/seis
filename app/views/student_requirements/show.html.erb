<h1>REVIEWING STUDENT REQUIREMENT</h1>
<div class="panel panel-default">
  <div class="panel-body">
<p id="notice"><%= notice %></p>

<p>
  <strong>Student:</strong>
  <%= @student_requirement.student_id %>
</p>

<p>
  <strong>Requirement:</strong>
  <%= @student_requirement.requirement_id %>
</p>

<p>
  <strong>Status:</strong>
  <%= @student_requirement.status %>
</p>
</div>
</div>

<%= link_to 'Edit', edit_student_requirement_path(@student_requirement), :class=>"btn btn-small btn-success" %> 
<%= link_to 'Back', student_requirements_path, :class=>"btn btn-small btn-success" %>
