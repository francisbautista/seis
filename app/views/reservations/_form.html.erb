<%= simple_form_for([@student, @reservation]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :remarks %>
    <%= f.input :amount %>
    <%= f.input :student_id %>
    <%= f.input :or_number %>
    <hr>
    <%= f.input :paid_status %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
