<div class="panel panel-success">
    <div class="panel-heading">
        <h2 class="panel-title"><h3>Test Permits</h3></h2>
    </div>
    <div class="panel-body">
        <table class="table table-striped">
          <thead>
            <tr>
              <th>Date</th>
              <th>Level</th>
              <th>Test Date</th>
              <th>Test Number</th>
              <th>Test Status</th>
              <th>Student</th>
              <th>Amount</th>
              <th>Paid Status</th>
              <th colspan="3"></th>
            </tr>
          </thead>

          <tbody>
            <% @tpermits.each do |tpermit| %>
              <tr>
                <td><%= tpermit.date %></td>
                <td><%= tpermit.level %></td>
                <td><%= tpermit.test_date %></td>
                <td><%= tpermit.test_number %></td>
                <td><%= tpermit.status %></td>
                <td><%= tpermit.student_id %></td>
                <td><%= tpermit.amount %></td>
                <td><%= tpermit.paid_status %></td>

              </tr>
            <% end %>
          </tbody>
        </table>
</div>
</div>

<%= link_to 'New Test Permit', new_student_tpermit_path, :class=>"btn btn-small btn-success" %>
