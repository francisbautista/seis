<%= simple_form_for(@admission) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :last_name %>
    <%= f.input :first_name %>
    <%= f.input :middle_name %>
    <%= f.input :grade_level %>
    <%= f.input :address %>
    <%= f.input :mode %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
