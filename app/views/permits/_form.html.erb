<%= simple_form_for(@permit) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :date %>
    <%= f.input :level %>
    <%= f.input :test_date %>
    <%= f.input :release %>
    <%= f.input :report_card %>
    <%= f.input :moral_cert %>
    <%= f.input :recommendation %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
